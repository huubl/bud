{
  "name": "@roots/bud-cli",
  "description": "CLI tools for Bud.",
  "version": "3.2.0-next.2",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "packages/@roots/bud-cli"
  },
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "license": "MIT",
  "contributors": [
    "Kelly Mears <kelly@roots.io> (https://kellymears.me)"
  ],
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "webpack",
    "bud"
  ],
  "bin": {
    "bud": "bin/bud"
  },
  "files": [
    "lib/",
    "bin/"
  ],
  "oclif": {
    "commands": "./lib/cjs/commands",
    "bin": "bud",
    "plugins": [
      "@oclif/plugin-plugins",
      "@oclif/plugin-update",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-which",
      "@oclif/plugin-commands",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help"
    ],
    "topics": {
      "build": {
        "description": "compile project assets"
      },
      "build:dev": {
        "description": "compile project assets"
      },
      "build:production": {
        "description": "compile project assets"
      }
    }
  },
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "types": "./lib/types/index.d.ts",
  "scripts": {
    "build": "yarn g:build",
    "build:cjs": "yarn g:build:cjs",
    "build:esm": "yarn g:build:esm",
    "lint": "yarn g:lint",
    "lint:eslint": "yarn g:lint:eslint",
    "lint:prettier": "yarn g:lint:prettier",
    "clean": "yarn g:clean",
    "clean:lib": "yarn g:clean:lib",
    "clean:modules": "yarn g:clean:modules",
    "clean:types": "yarn g:clean:types",
    "pkg:check": "yarn g:pkg:check",
    "pkg:format": "yarn g:pkg:format"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/fs-extra": "^9.0.11",
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.0",
    "@types/react": "^17.0.4",
    "@types/rimraf": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@endemolshinegroup/cosmiconfig-typescript-loader": "^3.0.2",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-plugins": "^1.10.0",
    "@oclif/plugin-update": "^1.3.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@oclif/plugin-which": "^1.0.3",
    "@roots/bud": "workspace:*",
    "@skypack/package-check": "^0.2.2",
    "autobind-decorator": "^2.4.0",
    "conf": "^10.0.1",
    "conf-cli": "^0.1.9",
    "cosmiconfig": "^7.0.0",
    "fs-extra": "^9.1.0",
    "globby": "^11.0.3",
    "lodash": "^4.17.21",
    "tslib": "^2.2.0"
  }
}
