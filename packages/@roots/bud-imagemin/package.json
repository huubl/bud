{
  "name": "@roots/bud-imagemin",
  "version": "3.2.0-next.2",
  "description": "Adds imagemin support to Bud",
  "repository": {
    "type": "git",
    "url": "git://github.com/roots/bud.git",
    "directory": "packages/@roots/bud-imagemin"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "license": "MIT",
  "keywords": [
    "bud",
    "bud-extension",
    "imagemin"
  ],
  "engines": {
    "node": ">=12"
  },
  "files": [
    "lib/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "types": "./lib/types/index.d.ts",
  "scripts": {
    "build": "run-p build:cjs build:esm",
    "build:cjs": "tsc --build tsconfig.json",
    "build:esm": "tsc --build tsconfig-esm.json",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "lint:prettier": "prettier 'src/**/*'  --write",
    "clean": "run-s clean:*",
    "clean:lib": "rimraf ./lib",
    "clean:modules": "rimraf ./node_modules",
    "clean:types": "rimraf ./types",
    "pkg": "run-s pkg:*",
    "pkg:check": "package-check --verbose",
    "pkg:format": "prettier './lib/**/*.{js,ts}' --write"
  },
  "peerDependencies": {
    "@roots/bud-framework": "^3.1.0",
    "@roots/bud-support": "^3.1.0"
  },
  "devDependencies": {
    "@roots/bud-framework": "^3.2.0-next.2",
    "webpack": "^5.31.2"
  },
  "dependencies": {
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^9.0.0"
  },
  "gitHead": "2895cea534566264c5a908ad52b26a2eab1e5684"
}
