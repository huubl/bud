{
  "name": "@roots/bud-babel",
  "version": "3.2.0-next.2",
  "description": "A friendly build tool to help manage your project assets.",
  "homepage": "https://roots.io/bud",
  "repository": {
    "type": "git",
    "url": "https://github.com/roots/bud.git",
    "directory": "packages/@roots/bud-babel"
  },
  "bugs": {
    "url": "https://github.com/roots/bud/issues"
  },
  "funding": {
    "type": "github sponsors",
    "url": "https://github.com/sponsors/roots"
  },
  "license": "MIT",
  "contributors": [
    "Kelly Mears <kelly@roots.io> (https://kellymears.me)"
  ],
  "keywords": [
    "bud",
    "bud-extension",
    "babel"
  ],
  "engines": {
    "node": ">=12"
  },
  "files": [
    "lib/"
  ],
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "exports": {
    "require": "./lib/cjs/index.js",
    "import": "./lib/esm/index.js"
  },
  "types": "./lib/types/index.d.ts",
  "scripts": {
    "build": "yarn g:build",
    "build:cjs": "yarn g:build:cjs",
    "build:esm": "yarn g:build:esm",
    "lint": "yarn g:lint",
    "lint:eslint": "yarn g:lint:eslint",
    "lint:prettier": "yarn g:lint:prettier",
    "clean": "yarn g:clean",
    "clean:lib": "yarn g:clean:lib",
    "clean:modules": "yarn g:clean:modules",
    "clean:types": "yarn g:clean:types",
    "pkg:check": "yarn g:pkg:check",
    "pkg:format": "yarn g:pkg:format"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.0",
    "@types/rimraf": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4",
    "webpack": "^5.36.0"
  },
  "dependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@roots/bud-framework": "workspace:*",
    "@roots/bud-support": "workspace:*",
    "@skypack/package-check": "^0.2.2",
    "autobind-decorator": "^2.4.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "lodash": "^4.17.21"
  }
}
